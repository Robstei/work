array<string> letters[5] = {"a","b","c","d","e"};
letters.shuffle();


term.print_line(letters);

loop int i = 1 until i >letters.count()
begin
	
	se.set_target_button(0);
	if(letters[i] == "a")
	then
		se.set_target_button(1);
	else
		se.set_response_active(true);
	end;
	
	test_text.set_caption(letters[i]);
	test_text.redraw();
	se.set_event_code(letters[i]);
	trial_test.present();
	i = i + 1;
end;